v class=\"right\">\r\n                    <el-input placeholder=\" 请输入用户名称\" v-model=\"input\"  style=\"width:250px\">\r\n                        <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"serch()\"></el-button>\r\n                    </el-input>\r\n                </div>\r\n            </div>\r\n            <div class=\"list\">\r\n                <el-table :data=\"userlist\" stripe border style=\"width: 100%\">\r\n                    <el-table-column type=\"index\" width=\"80\" label=\"序号\">\r\n                    </el-table-column>\r\n                    <el-table-column label=\"头像\" width=\"120\">\r\n                        <template slot-scope=\"scope\">\r\n                            <img :src=\"scope.row.user_portrait\" alt=\"\" class=\"header\">\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"user_nickname\" label=\"名称\" width=\"100\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"user_sex\" label=\"性别\" width=\"130\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"user_phone\" label=\"联系电话\" width=\"180\">\r\n                    </el-table-column>\r\n                    <el-table-column label=\"区域\" >\r\n                        <template slot-scope=\"scope\">\r\n                            <p>{{scope.row.user_province}}{{scope.row.user_city}}{{scope.row.user_district}}</p>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"消费金额\" width=\"150\">\r\n                        <template slot-scope=\"scope\">\r\n                            <p>&yen;{{scope.row.consume_money}}</p>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"级别\" width=\"120\">\r\n                        <template slot-scope=\"scope\">\r\n                            <p>LV{{scope.row.user_level}}</p>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"订单数\" width=\"100\">\r\n                        <template slot-scope=\"scope\">\r\n                            <router-link :to=\"{name:'studentsorder',query:{id:scope.row.user_id}}\" class=\"link\">{{scope.row.student_order_number}}</router-link>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"created_at\" label=\"注册时间\" width=\"100\">\r\n                    </el-table-column>\r\n                    <el-table-column label=\"操作\" width=\"120\" >\r\n                        <template slot-scope=\"scope\">\r\n                            <div class=\"btn del\" @click=\"ban(scope.row.user_id,2)\" v-if=\"scope.row.is_disable==1\">解禁</div>\r\n                            <div class=\"btn active\" @click=\"ban(scope.row.user_id,1)\" v-else>禁用</div>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </div>\r\n            <el-pagination\r\n                @current-change=\"handleCurrentChange\"\r\n                :page-size=\"10\"\r\n                layout=\"total, prev, pager, next\"\r\n                :total=\"total_page\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            input:'',\r\n            total_page:1,\r\n            options: [\r\n                {\r\n                value: '1',\r\n                label: '初级'\r\n                }, \r\n                {\r\n                value: '2',\r\n                label: '中级'\r\n                }, \r\n                {\r\n                value: '3',\r\n                label: '高级'\r\n                }, \r\n                \r\n            ],\r\n            userinfo:{},\r\n            cove:require(\"../../assets/logo.png\"),\r\n            teachername:'xxx',\r\n            value: '',\r\n            id:'',\r\n            userlist:[],\r\n        }\r\n    },\r\n    created(){\r\n        this.id=this.$route.query.id\r\n        this.getteacherstudentslist(1)\r\n    },\r\n    methods:{\r\n        getteacherstudentslist(page){\r\n            const url = this.$host + `admin/service_personnel/my_team`;\r\n            //console.log(url)\r\n            this.axios.get(url,{\r\n               params:{\r\n                    pageSize:10,\r\n                    page:page,\r\n                    course_instructor_id:this.id,\r\n                    keyword:this.input\r\n                }\r\n              }).then(res=>{\r\n                console.log(res);\r\n                if(res.status == 200){\r\n                    let resData = res.data;\r\n                    this.userlist=resData.result\r\n                    this.total_page=resData.total_number\r\n                    this.userinfo=resData.user_info\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n            })\r\n        },\r\n        ban(id,i){\r\n            var data = {\r\n                user_id:id,\r\n                mtype:i,\r\n            }\r\n            const url=this.$host + `admin/service_personnel/forbid_account`\r\n            this.axios.post(url,data).then(res=>{\r\n                  console.log(res)\r\n                  var resData=res.data\r\n                  if(resData.code==1){\r\n                      this.$message({\r\n                        message:resData.msg,\r\n                        type: 'success'\r\n                    });\r\n                  }\r\n                  this.getteacherstudentslist(1)\r\n                }).catch(error=>{\r\n                    console.log(error);\r\n             })\r\n        },\r\n        serch(){\r\n            this.getteacherstudentslist(1)\r\n        },\r\n        handleCurrentChange(val) {\r\n           this.getteacherstudentslist(val)\r\n        },\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.userlist{\r\n    .content{\r\n        height: auto;\r\n        .top{\r\n            height: 80px;\r\n            .left{\r\n                font-size: 16px;\r\n                img{\r\n                    width: 48px;\r\n                    height: 48px;\r\n                    border-radius:50%;\r\n                    margin-right: 20px;\r\n                }\r\n            }\r\n            \r\n        }\r\n    }\r\n}\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/pages/teacherManagement/userlist.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"userlist\"},[_c('div',{staticClass:\"content bg-f padding-24\"},[_c('div',{staticClass:\"top space-between\"},[_c('div',{staticClass:\"center-col left\"},[_c('img',{attrs:{\"src\":_vm.userinfo.user_portrait,\"alt\":\"\"}}),_vm._v(\" \"),_c('p',{staticClass:\"fs-18\"},[_vm._v(_vm._s(_vm.userinfo.user_nickname))])]),_vm._v(\" \"),_c('div',{staticClass:\"right\"},[_c('el-input',{staticStyle:{\"width\":\"250px\"},attrs:{\"placeholder\":\" 请输入用户名称\"},model:{value:(_vm.input),callback:function ($$v) {_vm.input=$$v},expression:\"input\"}},[_c('el-button',{attrs:{\"slot\":\"append\",\"icon\":\"el-icon-search\"},on:{\"click\":function($event){return _vm.serch()}},slot:\"append\"})],1)],1)]),_vm._v(\" \"),_c('div',{staticClass:\"list\"},[_c('el-table',{staticStyle:{\"width\":\"100%\"},attrs:{\"data\":_vm.userlist,\"stripe\":\"\",\"border\":\"\"}},[_c('el-table-column',{attrs:{\"type\":\"index\",\"width\":\"80\",\"label\":\"序号\"}}),_vm._v(\" \"),_c('el-table-column',{attrs:{\"label\":\"头像\",\"width\":\"120\"},scopedSlots:_vm._u([{key:\"default\",fn:function(scope){return [_c('img',{staticClass:\"header\",attrs:{\"src\":scope.row.user_portrait,\"alt\":\"\"}})]}}])}),_vm._v(\" \"),_c('el-table-column',{attrs:{\"prop\":\"user_nickname\",\"label\":\"名称\",\"width\"