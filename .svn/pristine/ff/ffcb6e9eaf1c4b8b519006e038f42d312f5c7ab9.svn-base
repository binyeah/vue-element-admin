data=\"teacherlist\" stripe border style=\"width: 100%\">\r\n                    <el-table-column type=\"index\" width=\"50\" label=\"序号\">\r\n                    </el-table-column>\r\n                    <el-table-column label=\"头像\" width=\"80\">\r\n                        <template slot-scope=\"scope\">\r\n                            <img :src=\"scope.row.user_portrait\" alt=\"\" class=\"header\">\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"名称\" width=\"100\">\r\n                        <template slot-scope=\"scope\">\r\n                            <p>{{scope.row.user_real_name||scope.row.user_nickname}}</p>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"user_phone\" label=\"联系电话\" width=\"130\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"service_type\" label=\"服务类型\" width=\"150\">\r\n                        <!-- <template slot-scope=\"scope\">\r\n                            <span v-for=\"(item,index) in scope.row.kinds\" :key=\"index\">{{item}};</span>\r\n                        </template> -->\r\n                    </el-table-column>\r\n                    <el-table-column label=\"服务地区\" >\r\n                        <template slot-scope=\"scope\">\r\n                            <span v-for=\"(item,index) in scope.row.user_service_region\" :key=\"index\">{{item[1]+item[2]}};</span>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"学员\" width=\"50\">\r\n                        <template slot-scope=\"scope\">\r\n                            <router-link :to=\"{name:'userlist',query:{id:scope.row.user_id}}\" class=\"link\">{{scope.row.student_number}}</router-link>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"订单数\" width=\"80\">\r\n                        <template slot-scope=\"scope\">\r\n                            <router-link :to=\"{name:'selforder',query:{id:scope.row.user_id}}\" class=\"link\">{{scope.row.order_number}}</router-link>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"发布的课程数\"  width=\"120\">\r\n                        <template slot-scope=\"scope\">\r\n                            <router-link :to=\"{name:'selfcoure',query:{id:scope.row.user_id}}\" class=\"link\">{{scope.row.course_number}}</router-link>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"order_amount\" label=\"订单金额\" width=\"100\">\r\n                    </el-table-column>\r\n                    <el-table-column label=\"状态\" width=\"100\">\r\n                        <template slot-scope=\"scope\">\r\n                            <p>{{scope.row.order_status==0?'未知':(scope.row.order_status==1?'接单中':'暂停接单')}}</p>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"好评率\" width=\"80\">\r\n                        <template slot-scope=\"scope\">\r\n                            <p>{{scope.row.good_rate}}%</p>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"级别\" width=\"100\">\r\n                        <template slot-scope=\"scope\">\r\n                            <!-- <input type=\"text\" v-model=\"scope.row.level\" class=\"sort\" style=\"width:50px\"> -->\r\n                            <el-select v-model=\"scope.row.coach_level\" placeholder=\"请选择\" @change=\"chooselevel(scope.row)\">\r\n                                <el-option v-for=\"item in level\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n                                </el-option>\r\n                            </el-select>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"操作\" width=\"120\" >\r\n                        <template slot-scope=\"scope\">\r\n                            <div class=\"btn del\" @click=\"ban(scope.row.user_id,2)\" v-if=\"scope.row.is_disable==1\">解禁</div>\r\n                            <div class=\"btn active\" @click=\"ban(scope.row.user_id,1)\" v-else>禁止账号</div>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </div>\r\n            <el-pagination\r\n                @current-change=\"handleCurrentChange\"\r\n                :page-size=\"10\"\r\n                layout=\"total, prev, pager, next\"\r\n                :total=\"total_page\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            input:'',\r\n            total_page:1,\r\n            options: [\r\n                {\r\n                value: '选项1',\r\n                label: '黄金糕'\r\n                }, \r\n                {\r\n                value: '选项2',\r\n                label: '双皮奶'\r\n                }, \r\n                {\r\n                value: '选项3',\r\n                label: '蚵仔煎'\r\n                }, \r\n                {\r\n                value: '选项4',\r\n                label: '龙须面'\r\n                }, \r\n                {\r\n                value: '选项5',\r\n                label: '北京烤鸭'\r\n                }\r\n            ],\r\n            value: '',\r\n            teacherlist:[],\r\n            level:[\r\n                {\r\n                    value:'0',\r\n                    label:'未知'\r\n                },\r\n                {\r\n                    value:'1',\r\n                    label:'LV1'\r\n                },\r\n                {\r\n                    value:'2',\r\n                    label:'LV2'\r\n                },\r\n                {\r\n                    value:'3',\r\n                    label:'LV3'\r\n                }\r\n            ]\r\n        }\r\n    },\r\n    created(){\r\n        this.getteacherlist(1)\r\n    },\r\n    methods:{\r\n        getteacherlist(page){\r\n            const url = this.$host + `admin/service_personnel/yoga_manage`;\r\n            //console.log(url)\r\n            this.axios.get(url,{\r\n               params:{\r\n                    pageSize:10,\r\n                    page:page,\r\n                    keyword:this.input,\r\n                    sid:this.value,\r\n                }\r\n              }).then(res=>{\r\n                console.log(res);\r\n                if(res.status == 200){\r\n                    let resData = res.data;\r\n                    // this.$message({\r\n                    //     message: '恭喜你，这是一条成功消息',\r\n                    //     type: 'success'\r\n                    // });\r\n                    this.teacherlist=resData.result\r\n                    this.total_page=resData.total_number\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n            })\r\n        },\r\n        choose(){\r\n            this.getteacherlist(1)\r\n        },\r\n        ban(id,i){\r\n            var data = {\r\n                user_id:id,\r\n                mtype:i,\r\n            }\r\n            const url=this.$host + `admin/service_personnel/forbid_account`\r\n            this.axios.post(url,data).then(res=>{\r\n                  console.log(res)\r\n                  var resData=res.data\r\n                  if(resData.code==1){\r\n                      this.$message({\r\n                        message:resData.msg,\r\n                        type: 'success'\r\n                    });\r\n                  }\r\n                  this.getteacherlist(1)\r\n                }).catch(error=>{\r\n                    console.log(error);\r\n             })\r\n        },\r\n        serch(){\r\n            this.getteacherlist(1)\r\n        },\r\n        chooselevel(i){\r\n            console.log(i)\r\n            var data = {\r\n                user_id:i.user_id,\r\n                coach_level:i.coach_level,\r\n            }\r\n            const url=this.$host + `admin/set_coach_level`\r\n            this.axios.post(url,data).then(res=>{\r\n                  console.log(res)\r\n                  var resData=res.data\r\n                  if(resData.code==1){\r\n                      this.$message({\r\n                        message:resData.msg,\r\n                        type: 'success'\r\n                    });\r\n                  }\r\n                  this.getteacherlist(1)\r\n                }).catch(error=>{\r\n                    console.log(error);\r\n             })\r\n        },\r\n        handleCurrentChange(val) {\r\n            //console.log(`当前页: ${val}`);\r\n            this.getteacherlist(val)\r\n        },\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.yoga{\r\n    .content{\r\n        height: auto;\r\n        .top{\r\n            height: 80px; \r\n        }\r\n    }\r\n}\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/pages/teacherManagement/yoga.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"yoga\"},[_c('div',{staticClass:\"content bg-f padding-24\"},[_c('div',{staticClass:\"top space-between\"},[_c('div',{staticClass:\"center-col left\"},[_c('el-input',{staticStyle:{\"width\":\"250px\"},attrs:{\"placeholder\":\" 请输入您要搜索的内容\"},model:{value:(_vm.input),callback:function ($$v) {_vm.input=$$v},expression:\"input\"}},[_c('el-button',{attrs:{\"slot\":\"append\",\"icon\":\"el-icon-search\"},on:{\"click\":function($event){return _vm.serch()}},slot:\"append\"})],1),_vm._v(\" \"),_c('div',{staticClass:\"center-col\",staticStyle:{\"margin-left\":\"20px\"}},[_c('p',[_vm._v(\"服务类型：\")]),_vm._v(\" \")