  \r\n                </el-form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            ruleForm:{\r\n                money:'',\r\n                day:'',\r\n                id:''\r\n            },\r\n            rules: {\r\n                money: [\r\n                    { required: true, message: '请输入提现额度', trigger: 'blur' },\r\n                ],\r\n                day: [\r\n                    { required: true, message: '请输入结算天数', trigger: 'blur' },\r\n                ],\r\n               \r\n            }\r\n        }\r\n    },\r\n    created(){\r\n        this.getwithdrawdata()\r\n    },\r\n    methods:{\r\n        getwithdrawdata(){\r\n            const url=this.$host + `admin/withdraw/get_set_withdraw`\r\n           \r\n            this.axios.get(url).then(res=>{\r\n                console.log(res)\r\n                if(res.status==200){\r\n                    var resData=res.data\r\n                    this.ruleForm.money=resData.withdraw_amount\r\n                    this.ruleForm.day=resData.settlement_days\r\n                    this.ruleForm.id=resData.id\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n                }).catch(error=>{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                    console.log(error);\r\n            })\r\n        },\r\n        save(){\r\n            var data={\r\n                withdraw_amount:this.ruleForm.money,\r\n                settlement_days:this.ruleForm.day,\r\n                id:this.ruleForm.id\r\n            }\r\n            const url=this.$host + `admin/withdraw/set_withdraw`\r\n           \r\n            this.axios.post(url,data).then(res=>{\r\n                console.log(res)\r\n                if(res.status==200){\r\n                    if(res.data.code==1){\r\n                        this.$message({\r\n                            message:res.data.msg,\r\n                            type: 'success'\r\n                        });\r\n                        this.getwithdrawdata()\r\n                    }else{\r\n                        this.$message.error(res.data.msg);\r\n                    }\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n                }).catch(error=>{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                    console.log(error);\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.setwithdrawal{\r\n    .content{\r\n        height: auto;\r\n        .top{\r\n            height: 80px;\r\n            border-bottom: 1px solid #e9e9e9;\r\n            .save{\r\n                width: 60px;\r\n                height: 30px;\r\n                line-height: 30px;\r\n                text-align: center;\r\n                border-radius: 5px;\r\n            }\r\n            \r\n        }\r\n        .tabel{\r\n            padding-bottom: 20px;\r\n            padding-top: 20px;\r\n        }\r\n    }\r\n}\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/pages/withdrawalManagement/setwithdrawal.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"setwithdrawal\"},[_c('div',{staticClass:\"content bg-f\"},[_c('div',{staticClass:\"top space-between  padding-24\"},[_c('p',{staticClass:\"fs-16\"},[_vm._v(\"提现设置\")]),_vm._v(\" \"),_c('div',{staticClass:\"bg-active color-f save btn\",on:{\"click\":_vm.save}},[_vm._v(\"保存\")])]),_vm._v(\" \"),_c('div',{staticClass:\"tabel padding-24\"},[_c('el-form',{ref:\"ruleForm\",staticClass:\"demo-ruleForm\",attrs:{\"model\":_vm.ruleForm,\"rules\":_vm.rules,\"label-width\":\"200px\"}},[_c('el-form-item',{attrs:{\"label\":\"提现额度（元）：\",\"prop\":\"money\"}},[_c('el-input',{staticStyle:{\"width\":\"200px\"},model:{value:(_vm.ruleForm.money),callback:function ($$v) {_vm.$set(