mn label=\"课程内容\">\r\n                        <template slot-scope=\"scope\">\r\n                            <div class=\"center-col\">\r\n                                <img :src=\"scope.row.course_cover?scope.row.course_cover[0]:noimg\" alt=\"\" class=\"sq\">\r\n                                <p style=\"margin-left:10px\">{{scope.row.course_name}}</p>\r\n                            </div>\r\n                            \r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"评价星级\" width=\"160\">\r\n                        <template slot-scope=\"scope\">\r\n                            <div class=\"space-around\">\r\n                                <evaluaStar :defaultRating.default='Number(scope.row.star_level)' :readOnly.default='click'></evaluaStar>\r\n                                <p class=\"color-active\">{{scope.row.star_level}}分</p>\r\n                            </div>\r\n                            \r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column label=\"评价内容\" width=\"150\">\r\n                        <template slot-scope=\"scope\">\r\n                            <p class=\"color-active btn\" @click=\"look(scope.row)\">查看详情</p>\r\n                        </template>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"created_at\" label=\"评价时间\" width=\"180\">\r\n                    </el-table-column>\r\n                    \r\n                    \r\n                    <el-table-column label=\"操作\" width=\"150\">\r\n                        <template slot-scope=\"scope\">\r\n                            <div class=\"btn active\" @click=\"hidden(scope.row.id)\">隐藏</div>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </div>\r\n            <el-pagination\r\n                @current-change=\"handleCurrentChange\"\r\n                :page-size=\"10\"\r\n                layout=\"total, prev, pager, next\"\r\n                :total=\"total_page\">\r\n            </el-pagination>\r\n        </div>\r\n        <div class=\"addbox\">\r\n            <el-dialog title=\"评价详情\" :visible.sync=\"evaluationbox\">\r\n                <p>评价内容：{{evaluat.evaluation_content}}</p>\r\n                <div>\r\n                    <img :src=\"item\" alt=\"\" v-for=\"(item,index) in evaluat.evaluation_img\" :key=\"index\" class=\"img\" @click=\"lookimg(item)\">\r\n                </div>\r\n            </el-dialog>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            noimg:require(\"../../assets/timg.jpg\"),\r\n            total_page:1,\r\n            input:'',\r\n            click:true,\r\n            evaluat:'',\r\n            evaluationbox:false,\r\n            evaluationlist:[]\r\n        }\r\n    },\r\n    created(){\r\n        this.getevaluationlist(1)\r\n    },\r\n    methods:{\r\n        getevaluationlist(page){\r\n            const url=this.$host + `admin/course_evaluation`\r\n            this.axios.get(url,{\r\n               params:{\r\n                    pageSize:10,\r\n                    page:page,\r\n                    keyword:this.input,\r\n                }\r\n              }).then(res=>{\r\n                    console.log(res)\r\n                    this.evaluationlist=res.data.result\r\n                    this.total_page=res.data.total_number\r\n                }).catch(error=>{\r\n                    console.log(error);\r\n             })\r\n        },\r\n        serch(){\r\n            this.getevaluationlist(1)\r\n        },\r\n        hidden(i){\r\n            var data={\r\n                course_evaluation_id:i\r\n            }\r\n            const url=this.$host + `admin/course_evaluation/hidden_evaluation`\r\n            this.axios.post(url,data).then(res=>{\r\n                    console.log(res)\r\n                    var resData=res.data\r\n                    if(resData.code==1){\r\n                        this.$message({\r\n                            message:resData.msg,\r\n                            type: 'success'\r\n                        });\r\n                        this.getevaluationlist(1)\r\n                    }\r\n                }).catch(error=>{\r\n                    console.log(error);\r\n             })\r\n        },\r\n        handleCurrentChange(val) {\r\n            this.getevaluationlist(val)\r\n            //console.log(`当前页: ${val}`);\r\n        },\r\n        look(i){\r\n            //console.log(i)\r\n            this.evaluat=i\r\n            this.evaluationbox=true\r\n        },\r\n        lookimg(i){\r\n            window.open(i, '_blank');\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.evaluation{\r\n    .content{\r\n        height: auto;\r\n        padding-bottom: 20px;\r\n        .top{\r\n            height: 80px;\r\n            .add{\r\n                \r\n                width: 87px;\r\n                height: 30px;\r\n                line-height: 30px;\r\n                border-radius: 6px;\r\n                \r\n                text-align: center;\r\n            }\r\n        }\r\n    }\r\n    .addbox{\r\n        display: flex;\r\n        img{\r\n            width: 120px;\r\n            height: 120px;\r\n            margin: 0 10px 10px 0;\r\n        }\r\n    }\r\n}\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/pages/orderManagement/evaluation.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"evaluation\"},[_c('div',{staticClass:\"content bg-f padding-24\"},[_c('div',{staticClass:\"top space-between\"},[_c('div',{staticClass:\"center-col left\"},[_c('el-input',{staticStyle:{\"width\":\"250px\"},attrs:{\"placeholder\":\" 请输入您要搜索的内容\"},model:{value:(_vm.input),callback:function ($$v) {_vm.input=$$v},expressio