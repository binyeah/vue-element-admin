\r\n                        <template slot-scope=\"scope\">\r\n                            <div class=\"btn del\" @click=\"del(scope.row.id)\">删除</div>\r\n                           \r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </div>\r\n            <div class=\"addbox\">\r\n                <el-dialog title=\"添加时间\" :visible.sync=\"addbox\">\r\n                    <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"120px\" class=\"demo-ruleForm\">\r\n                        <el-form-item label=\"开始时间：\" prop=\"startime\">\r\n                            <el-input v-model=\"ruleForm.startime\" style=\"width:400px\" placeholder=\"请输入开始时间\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"结束时间：\" prop=\"endtime\">\r\n                            <el-input v-model=\"ruleForm.endtime\" style=\"width:400px\" placeholder=\"请输入结束时间\"></el-input>\r\n                        </el-form-item>\r\n                    </el-form>\r\n                    <div slot=\"footer\" class=\"dialog-footer\">\r\n                    <el-button @click=\"addbox = false\">取 消</el-button>\r\n                    <el-button type=\"primary\" @click=\"sure()\">确 定</el-button>\r\n                    </div>\r\n                </el-dialog>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            addbox:false,\r\n            timelist:[],\r\n            ruleForm:{\r\n                startime:'',\r\n                endtime:'',\r\n            },\r\n            rules: {\r\n                startime: [\r\n                    { required: true, message: '请输入开始时间', trigger: 'blur' },\r\n                ],\r\n                endtime: [\r\n                    { required: true, message: '请输入结束时间', trigger: 'blur' },\r\n                ],\r\n            }\r\n        }\r\n    },\r\n    created(){\r\n        this.gettimelist(1)\r\n    },\r\n    methods:{\r\n        gettimelist(page){\r\n            const url = this.$host + `admin/course_class_hour`;\r\n            //console.log(url)\r\n            this.axios.get(url,{\r\n               params:{\r\n                    pageSize:20,\r\n                    page:page,\r\n                }\r\n              }).then(res=>{\r\n                console.log(res);\r\n                if(res.status == 200){\r\n                    let resData = res.data;\r\n                    this.timelist=resData.result\r\n                    //this.total_page=resData.total_number\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n            })\r\n        },\r\n        del(i){\r\n            var data={\r\n                class_hour_id:i\r\n            }\r\n            //console.log(data)\r\n            const url = this.$host + `admin/course_class_hour/delete_class_hour`;\r\n            //console.log(url)\r\n            this.axios.post(url,data).then(res=>{\r\n                //console.log(res);\r\n                if(res.status == 200){\r\n                    let resData = res.data;\r\n                    if(resData.code==1){\r\n                        this.$message({\r\n                        message: '删除成功',\r\n                        type: 'success'\r\n                    });\r\n                    this.gettimelist(1)\r\n                    //this.addbox=false;\r\n                    }\r\n                    \r\n                    //this.total_page=resData.total_number\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n            })\r\n        },\r\n        add(){\r\n            \r\n            const url = this.$host + `admin/course_class_hour/add_class_hour`;\r\n            //console.log(url)\r\n            this.axios.post(url).then(res=>{\r\n                //console.log(res);\r\n                if(res.status == 200){\r\n                    let resData = res.data;\r\n                    \r\n                        this.$message({\r\n                        message: '添加成功',\r\n                        type: 'success'\r\n                    });\r\n                    this.gettimelist(1)\r\n                    //this.addbox=false;\r\n                   \r\n                    \r\n                    //this.total_page=resData.total_number\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n            })\r\n            \r\n        },\r\n        edit(item){\r\n           var data={\r\n                class_hour_id:item.id,\r\n                start_date:item.start_date,\r\n                end_date:item.end_date\r\n            }\r\n            //console.log(data)\r\n            const url = this.$host + `admin/course_class_hour/edit_class_hour`;\r\n            //console.log(url)\r\n            this.axios.post(url,data).then(res=>{\r\n                console.log(res);\r\n                if(res.status == 200){\r\n                    let resData = res.data;\r\n                    if(resData.code==1){\r\n                        this.$message({\r\n                        message: '编辑成功',\r\n                        type: 'success'\r\n                    });\r\n                    this.gettimelist(1)\r\n                    //this.addbox=false;\r\n                    }\r\n                    \r\n                    //this.total_page=resData.total_number\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n            }) \r\n        }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.settime{\r\n    .content{\r\n        height: auto;\r\n        padding-bottom: 20px;\r\n        .top{\r\n            height: 80px;\r\n            .add{\r\n                \r\n                width: 87px;\r\n                height: 30px;\r\n                line-height: 30px;\r\n                border-radius: 6px;\r\n                \r\n                text-align: center;\r\n            }\r\n        }\r\n        .list{\r\n            input{\r\n                text-align: center\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/pages/coureManagement/settime.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"settime\"},[_c('div',{staticClass:\"content bg-f padding-24\"},[_c('div',{staticClass:\"top space-between\"},[_c('p',{staticClass:\"fs-16\"},[_vm._v(\"设置时间\")]),_vm._v(\" \"),_c('div',{staticClass:\"add btn color-f bg-active\",on:{\"click\":_vm.add}},[_vm._v(\"添加\")])]),_vm._v(\" \"),_c('div',{staticClass:\"list\"},[_c('el-table',{ref:\"multipleTable\",staticStyle:{\"width\":\"100%\"},attrs:{\"data\":_vm.timelist,\"stripe\":\"\",\"border\":\"\"}},[_c('el-table-column',{attrs:{\"label\":\"开始时间\"},scopedSlots:_vm._u([{key:\"default\",fn:function(scope){return [_c('input',{directives:[{name:\"model\",rawName:\"v-model\",value:(scope.row.start_date),expression:\"scope.row.start_date\"}],attrs:{\"type\":\"text\"},domProps:{\"value\":(scope.row.start_date)},on:{\"change\":function($event){return _vm.edit(scope.row)},\"input\":function($event){if($event.target.composing){ return; }_vm.$set(scope.row, \"start_date\", $event.target.value)}}})]}}])}),_vm._v(\" \"),_c('el-table-column',{attrs:{\"label\":\"结束时间\"},scopedSlots:_vm._u([{key:\"default\",fn:function(scope){return [_c('input',{directives:[{name:\"model\",rawName:\"v-model\",value:(scope.row.end_date),expression:\"scope.row.end_date\"}],attrs:{\"type\":\"text\"},domProps:{\"value\":(scope.row.end_date)},on:{\"change\":function($event){return _vm.edit(scope.row)},\"input\":function($event){if($event.target.composing){ return; }_vm.$set(scope.row, \"end_date\", $event.target.value)}}})]}}])}),_vm._v(\" \"),_c('el-table-column',{attrs:{\"label\":\"操作\"},scopedSlots:_vm._u([{