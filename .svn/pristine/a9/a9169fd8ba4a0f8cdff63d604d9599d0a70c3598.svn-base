 url = this.$host + `/www/yoga/admin/advert`;\r\n            //console.log(url)\r\n            this.axios.get(url).then(res=>{\r\n                console.log(res);\r\n                if(res.status == 200){\r\n                    let resData = res.data;\r\n                    // this.$message({\r\n                    //     message: '恭喜你，这是一条成功消息',\r\n                    //     type: 'success'\r\n                    // });\r\n                    this.advertise=resData\r\n                }else{\r\n                    this.$message.error('网络错误！请稍后重试！');\r\n                }\r\n            })\r\n        },\r\n        handleAvatarSuccess1(file) {\r\n            this.imageUrl = file.dest;\r\n            this.updataImg()\r\n            //console.log(this.imageUrl)\r\n        },\r\n        upimg(val){\r\n            //console.log(val)\r\n            this.skuId=val\r\n        },\r\n        updataImg(){\r\n            var data={\r\n                \"advert_id\":this.skuId,\r\n                \"advert_cover\":this.imageUrl\r\n            }\r\n            //console.log(data)\r\n            const url=this.$host + `/www/yoga/admin/advert/edit_advert`;\r\n            this.axios.post(url,data).then(res=>{\r\n                //console.log(res.data)\r\n                this.$message({\r\n                        message: '上传成功！',\r\n                        type: 'success'\r\n                    });\r\n                this.getAdvertiselist()\r\n            }).catch(error=>{\r\n                console.log(error);\r\n            })\r\n        },\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.advertise{\r\n    .content{\r\n        height: auto;\r\n        .top{\r\n            height: 80px;\r\n            .left{\r\n                font-size: 16px;\r\n                span{\r\n                    margin-left: 20px;\r\n                    font-size: 14px;\r\n                    color: #E51C23;\r\n\r\n                }\r\n            }\r\n        }\r\n        .imgbox{\r\n            wi