n            </div>\r\n            <div class=\"tabel padding-24\">\r\n                <div class=\"list\">\r\n                    <div class=\"center-col\">\r\n                        <span class=\"fs-18 color-r\">＊</span>\r\n                        <p>平台：教练</p>\r\n                        <input type=\"number\" name=\"\" id=\"\" v-model=\"inptut1\" @input=\"setone\">:<input type=\"number\" v-model=\"inptut2\" @input=\"setthree\">(例 3：7)\r\n                    </div>\r\n                    <div class=\"center-col\">\r\n                        <span class=\"fs-18 color-r\">＊</span>\r\n                        <p>平台：瑜伽馆</p>\r\n                        <input type=\"number\" name=\"\" id=\"\" v-model=\"inptut3\" @input=\"settwo\">:<input type=\"number\" v-model=\"inptut4\" @input=\"setfour\">(例 3：7)\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            id:'',\r\n            inptut1:0,\r\n            inptut2:0,\r\n            inptut3:0,\r\n            inptut4:0,\r\n        }\r\n    },\r\n    created(){\r\n        this.getsetratio()\r\n    },\r\n    methods:{\r\n        getsetratio(){\r\n            const url=this.$host + `admin/service_personnel/get_set_ratio`\r\n            this.axios.get(url).then(res=>{\r\n                //console.log(res)\r\n                var resData = res.data.data\r\n                this.id=resData.id\r\n                this.inptut1=resData.plat_coach[0]\r\n                this.inptut2=resData.plat_coach[1]\r\n                this.inptut3=resData.plat_yoga[0]\r\n                this.inptut4=resData.plat_yoga[1]\r\n                }).catch(error=>{\r\n                    console.log(error);\r\n            })\r\n        },\r\n        setone(){\r\n            this.inptut2=10-this.inptut1\r\n        },\r\n        settwo(){\r\n            this.inptut4=10-this.inptut3\r\n        },\r\n        setthree(){\r\n            this.inptut1=10-this.inptut2\r\n        },\r\n        setfour(){\r\n            this.inptut3=10-this.inptut4\r\n        },\r\n        save(){\r\n            if(this.inptut1<0||this.inptut2<0||this.inptut3<0||this.inptut4<0){\r\n                this.$message.error(\"数值不能小于0！\");\r\n                return\r\n            }\r\n            var data = {\r\n                id:this.id,\r\n                plat_coach:[this.inptut1,this.inptut2],\r\n                plat_yoga:[this.inptut3,this.inptut4]\r\n            }\r\n            console.log(data)\r\n            //return\r\n            const url=this.$host + `admin/service_personnel/set_ratio`\r\n            this.axios.post(url,data).then(res=>{\r\n                  //console.log(res)\r\n                  var resData=res.data\r\n                  if(resData.code==1){\r\n                      this.$message({\r\n                        message:resData.msg,\r\n                        type: 'success'\r\n                    });\r\n                    this.getsetratio()\r\n                  }else{\r\n                       this.$message.error(resData.msg);\r\n                  }\r\n                  \r\n                }).catch(error=>{\r\n                    console.log(error);\r\n             })\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.setEarningsratio{\r\n    .content{\r\n        height: auto;\r\n        .top{\r\n            height: 80px;\r\n            border-bottom: 1px solid #e9e9e9;\r\n            .save{\r\n                width: 60px;\r\n                height: 30px;\r\n                line-height: 30px;\r\n                text-align: center;\r\n                border-radius: 5px;\r\n            }\r\n            \r\n        }\r\n        .tabel{\r\n            min-height: 500px;\r\n            padding-bottom: 20px;\r\n            padding-top: 20px;\r\n            .list{\r\n                margin: 50px ;\r\n                .center-col{\r\n                    height: 40px;\r\n                    //background-color: greenyellow;\r\n                    margin-bottom: 10px;\r\n                    span{\r\n                        margin-top: 8px;\r\n                    }\r\n                    p{\r\n                        margin: 0 20px 0 10px;\r\n                    }\r\n                    input{\r\n                        width: 103px;\r\n                        height: 36px;\r\n                        line-height: 20px;\r\n                        text-align: center;\r\n                        outline: none;\r\n                        margin: 0 10px;\r\n                        border: 1px solid rgba(187, 187, 187, 1);\r\n                    }\r\n                }\r\n                \r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// src/pages/teacherManagement/setEarningsratio.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"setEarningsratio\"},[_c('div',{staticClass:\"content bg-f\"},[_c('div',{staticClass:\"top space-between  padding-24\"},[_c('p',{staticClass:\"fs-16\"},[_vm._v(\"结算比例设置\")]),_vm._v(\" \"),_c('div',{staticClass:\"bg-active color-f save btn\",on:{\"click\":_vm.save}},[_vm._v(\"保存\")])]),_vm._v(\" \"),_c('div',{staticClass:\"tabel padding-24\"},[_c('div',{staticClass:\"list\"},[_c('div',{staticClass:\"center-col\"},[_c('span',{staticClass:\"fs-18 color-r\"},[_vm._v(\"＊\")]),_vm._v(\" \"),_c('p',[_vm._v(\"平台：教练\")]),_vm._v(\" \"),_c('input',{directives:[{name:\"model\",rawName:\"v-model\",value:(_vm.inptut1),expression:\"inptut1\"}],attrs:{\"type\":\"number\",\"name\":\"\",\"id\":\"\"},domProps:{\"value\":(_vm.inptut1)},on:{\"input\":[function($event){if($event.target.composing){ return; }_vm.inptut1=$event.target.value},_vm.setone]}}),_vm._v(\":\"),_c('input',{directives:[{name:\"model\",rawName:\"v-model\",value:(_vm.inptut2),expression:\"inptut2\"}],attrs:{\"type\":\"number\"},domProps:{\"value\":(_vm.inptut