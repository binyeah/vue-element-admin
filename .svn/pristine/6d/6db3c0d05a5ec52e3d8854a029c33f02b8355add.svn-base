" label=\"序号\">\r\n                    </el-table-column>\r\n                    <!-- <el-table-column label=\"头像\" >\r\n                        <template slot-scope=\"scope\">\r\n                            <img :src=\"scope.row.img\" alt=\"\" class=\"header\">\r\n                        </template>\r\n                    </el-table-column> -->\r\n                    <el-table-column prop=\"admin_account\" label=\"账号\" >\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"admin_password\" label=\"密码\" >\r\n                    </el-table-column>\r\n                    <el-table-column label=\"操作\" >\r\n                        <template slot-scope=\"scope\">\r\n                            <div class=\"center-col\">\r\n                                <div class=\"btn active\" @click=\"edit(scope.row.id)\">编辑</div>\r\n                                <div class=\"btn del\" @click=\"del(scope.row.id)\" >删除</div>\r\n                            </div>\r\n                            \r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n            </div>\r\n            <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :page-size=\"10\"\r\n                layout=\"total, prev, pager, next\"\r\n                :total=\"total_page\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            input:'',\r\n            total_page:1,\r\n            accountlist:[\r\n                {\r\n                    img:require(\"../../assets/logo.png\"),\r\n                    num:'6874545674',\r\n                    psd:'684845dsfd',\r\n                    id:'845'\r\n                }\r\n            ]\r\n        }\r\n    },\r\n    created(){\r\n        this.getAccountList(1)\r\n    },\r\n    method